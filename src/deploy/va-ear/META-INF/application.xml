<?xml version="1.0" encoding="UTF-8"?>
<application version="5"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                        http://java.sun.com/xml/ns/javaee/application_5.xsd"
    >

    <description>OCSP responder</description>
    <display-name>OCSP responder</display-name>
    <module>
        <ejb>va-ejb.jar</ejb>
    </module>
    <module>
        <web>
            <web-uri>va.war</web-uri>
            <context-root>/ejbca/publicweb/status</context-root>
        </web>
    </module>
    <module>
        <web>
            <web-uri>healthcheck.war</web-uri>
            <context-root>/ejbca/publicweb/vahealthcheck</context-root>
        </web>
    </module>
</application>
