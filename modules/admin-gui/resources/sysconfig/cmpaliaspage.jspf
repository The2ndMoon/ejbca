<div align="center"> 
  <h2><c:out value="<%= ejbcawebbean.getText(\"CMPEDITALIAS\") %>" /></h2>
  <h3><c:out value="<%= ejbcawebbean.getText(\"CMPALIAS\")+ \" : \" + alias %>" /></h3>
</div>

<form name="cmpconfiguration" method="post" action="<%=THIS_FILENAME %>">
  <input type="hidden" name='<%= ACTION %>' value='<%=ACTION_EDIT_ALIAS %>'>
  <input type="hidden" name='<%= HIDDEN_ALIAS %>' value='<c:out value="<%=alias %>"/>'>
  <table class="edit" width="100%" border="0" cellspacing="3" cellpadding="3">
    <tr id="Row<%=row++%2%>"> 
      <td width="50%" valign="top"> 
        <div align="left"> 
          &nbsp;
        </div>
      </td>
      <td width="50%" valign="top" align="right"> 
        <a href="<%=THIS_FILENAME %>"><c:out value="<%= ejbcawebbean.getText(\"CMPBACKTOALIASES\") %>" /></a>
        <!-- &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        <a onclick='displayHelpWindow("<%= ejbcawebbean.getHelpfileInfix("ca_help.html") + "#certificateprofiles"%>")'>
        <c:out value="<%= ejbcawebbean.getText(\"HELP\") %>" /></a> -->
      </td>
    </tr>


    <!-- ---------------------- DEFAULT CMP CA ----------------------------- -->    
    <tr  id="Row<%=row++%2%>"> 
      <td width="50%" align="top"> 
        <h3><c:out value="<%= ejbcawebbean.getText(\"CMPDEFAULTCA\") %>" /></h3>
        <p class="help"><c:out value="Enforce a particual CA instead of taking it from the request."/></p>
      </td>
      <td width="50%"> 
        <select name="<%=LIST_CMPDEFAULTCAS%>" >
           <%  String currentCA = cmpconfig.getCMPDefaultCA(alias); %>
           <option <% if(currentCA.equals("")) { %> selected <% } %>
                 	value='<c:out value=""/>'><c:out value="(disabled)"/></option>
           <% java.util.Iterator iter = canames.keySet().iterator();
           	  while(iter.hasNext()) {
           		  String caname = (String) iter.next();
           		  String cadn = cabean.getCAInfo(caname).getCAInfo().getSubjectDN();
           %>     <option <% if(currentCA.equals(cadn)) { %> selected <% } %>
                  value='<c:out value="<%= caname%>"/>'><c:out value="<%= caname %>"/></option> 
           <% } %>
        </select>
      </td>
    </tr>

        

    
    <!-- ---------------------- CMP Operational mode ----------------------------- -->
    <tr  id="Row<%=row++%2%>"> 
      <td width="50%" align="top"> 
        <h3><c:out value="<%= ejbcawebbean.getText(\"CMPOPERATIONALMODE\") %>"/>
        <%= ejbcawebbean.getHelpReference("/adminguide.html#Client%20mode%20for%20CMP") %>
        </h3>
        <p class="help"><c:out value="If the CMP service should work in 'client' or 'ra' mode."/></p>
      </td>
      <td width="50%"> 
        <input type="radio" name="<%=RADIO_CMPMODE%>" value="client" onchange="changemode()" 
			<% 
				if(!cmpconfig.getRAMode(alias)) { 
					out.write(" CHECKED ");
					ramode = false;
				}
			%> />
		<label><c:out value="<%= ejbcawebbean.getText(\"CMPCLIENTMODE\") %>" /></label>
		&nbsp;&nbsp;&nbsp;&nbsp;
		<input type="radio" name="<%=RADIO_CMPMODE%>" value="ra" onchange="changemode()"  
			<% 
				if(cmpconfig.getRAMode(alias)) {
					out.write(" CHECKED ");
					ramode = true;
				}
			%> />
		<label><c:out value="<%= ejbcawebbean.getText(\"CMPRAMODE\") %>" /></label>
		<input type="submit" name="<%= BUTTON_RELOAD %>" id="<%= BUTTON_RELOAD %>" value="<%= ejbcawebbean.getText("RELOAD") %>" style="display:none">
		<noscript>
			&nbsp;&nbsp;&nbsp;&nbsp;
			<input type="submit" name="<%= BUTTON_RELOAD %>" id="<%= BUTTON_RELOAD %>" value="<%= ejbcawebbean.getText("RELOAD") %>">
		</noscript> 
      </td>
    </tr> 


    <!-- ---------------------- Response Protection ----------------------------- -->
    <tr  id="Row<%=row++%2%>"> 
      <td width="50%" align="top"> 
        <h3><c:out value="<%= ejbcawebbean.getText(\"CMPRESPONSEPROTECTION\") %>"/>
        <%= ejbcawebbean.getHelpReference("/adminguide.html#CMP%20Response%20Message") %>
        </h3>
        <p class="help"><c:out value="Which sort of protection the response messages will have."/></p>
      </td>
      <td width="50%"> 
        <select name="<%= LIST_CMPRESPONSEPROTECTION %>" onchange="checkresponseprotection(this)">
          <% String currentProtection = cmpconfig.getResponseProtection(alias); %>
          <% if(currentProtection.equals("pbe"))	pbe = true; %>
          <% java.util.Iterator iterp = cmpconfig.getCmpResponseProtectionList(ramode).iterator();
             while(iterp.hasNext()){
            	String p = (String) iterp.next(); 
          %>   <option <% if(p.equals(currentProtection)){ %> selected <% } %>
                  value='<c:out value="<%= p%>"/>'><c:out value="<%= p %>"/></option>
          <% } %>
        </select>
        <% if(ramode) { %>
       	<noscript>
			&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
			<input type="submit" name="<%= BUTTON_RELOAD %>" id="<%= BUTTON_RELOAD %>" value="<%= ejbcawebbean.getText("RELOAD") %>">
		</noscript>
		<% } %>
      </td>
    </tr>
    



    <!-- ---------------------- CMP Authentication Modules ----------------------------- -->    
    <tr  id="Row<%=row++%2%>"> 
      <td width="50%" align="top"> 
        <h3><c:out value="<%= ejbcawebbean.getText(\"CMPAUTHENTICATIONMODULE\") %>"/>
        <%= ejbcawebbean.getHelpReference("/adminguide.html#CMP%20Message%20Authentication") %>
        </h3>
        <p class="help"><c:out value="<%= ejbcawebbean.getText(\"CMPAUTHENTICATIONMODULE_HELP\") %>"/></p>
      </td>
      <td width="50%"> 
      	<input type="checkbox" name="<%= CHECKBOX_HMAC %>" value="<%=cmpconfig.AUTHMODULE_HMAC %>" 
				<% if(cmpconfig.isInAuthModule(alias, cmpconfig.AUTHMODULE_HMAC)) {
					out.write(" CHECKED ");
				}%>
				id="<%=CHECKBOX_HMAC%>" />
				<label for="<%=CHECKBOX_HMAC%>"><c:out value="<%= cmpconfig.AUTHMODULE_HMAC %>" /></label>
				<% if(ramode) {%>
					&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
					
					<% String secretvalue = cmpconfig.getAuthenticationParameter(cmpconfig.AUTHMODULE_HMAC, alias);	%>
					<input type="radio" name="<%=RADIO_HMACPASSWORD%>" value="casecret"  
						<% 
							if(secretvalue.equals("-") || secretvalue.length()==0) { 
								out.write(" CHECKED ");
								secretvalue = "";
							}
						%> />
					<label><c:out value="<%= ejbcawebbean.getText(\"CMPHMACCASECRET\") %>" /></label>
					&nbsp;&nbsp;&nbsp;
					<input type="radio" name="<%=RADIO_HMACPASSWORD%>" value="hmacsecret"  
						<% 
							if(! ( secretvalue.equals("-") || (secretvalue.length() == 0) ) ) { 
								out.write(" CHECKED ");
							}
						%> />
					<label><c:out value="<%= ejbcawebbean.getText(\"CMPHMACSPECIFYSECRET\") %>" /></label>
					<input type="text" name="<%=TEXTFIELD_HMACPASSWORD %>" value='<c:out value="<%= secretvalue %>"/>' 
        				maxlength="150" size="25" title="<%= ejbcawebbean.getText("FORMAT_STRING") %>">
        		<% } %>
        		<br/>
		<% if(!pbe) { %>
		<input type="checkbox" name="<%= CHECKBOX_EEC %>" value="<%=cmpconfig.AUTHMODULE_ENDENTITY_CERTIFICATE %>"
				<% if(cmpconfig.isInAuthModule(alias, cmpconfig.AUTHMODULE_ENDENTITY_CERTIFICATE)) {
						out.write(" CHECKED ");
					}%>
				id="<%=CHECKBOX_EEC%>" />
				<label for="<%=CHECKBOX_EEC%>"><c:out value="<%= cmpconfig.AUTHMODULE_ENDENTITY_CERTIFICATE %>" /></label>
				<% if(ramode) {%>
				&nbsp;&nbsp;&nbsp;
				<label for="<%=LIST_ECCCAS%>"><c:out value="<%= ejbcawebbean.getText(\"CMPECCISSUERCA\") %>" /></label>
				<select name="<%=LIST_ECCCAS%>" >
           				<%  String currentECCCA = cmpconfig.getAuthenticationParameter(cmpconfig.AUTHMODULE_ENDENTITY_CERTIFICATE, alias); %>
           				<% java.util.Iterator itrecc = canames.keySet().iterator();
           	  			while(itrecc.hasNext()) {
           		  			String eccca = (String) itrecc.next();
           			%>     <option <% if(currentECCCA.equals(eccca)) { %> selected <% } %>
                  				value='<c:out value="<%= eccca%>"/>'><c:out value="<%= eccca %>"/></option> 
           			<% } %>
        		</select>
        		<% } %>
        		<br/>
		<% } %>
		<% if(!ramode) { %>
		<input type="checkbox" name="<%= CHECKBOX_REGTOKEN %>" value="<%=cmpconfig.AUTHMODULE_REG_TOKEN_PWD %>" 
				<% if(cmpconfig.isInAuthModule(alias, cmpconfig.AUTHMODULE_REG_TOKEN_PWD)) out.write(" CHECKED ");	%>
				id="<%=CHECKBOX_REGTOKEN%>" />
				<label for="<%=CHECKBOX_REGTOKEN%>"><c:out value="<%= cmpconfig.AUTHMODULE_REG_TOKEN_PWD %>" /></label><br/>

		<input type="checkbox" name="<%= CHECKBOX_DNPART %>" value="<%=cmpconfig.AUTHMODULE_DN_PART_PWD %>"
				<% if(cmpconfig.isInAuthModule(alias, cmpconfig.AUTHMODULE_DN_PART_PWD)) {
						out.write(" CHECKED ");
					}%>
				id="<%=CHECKBOX_DNPART%>" />
				<label for="<%=CHECKBOX_DNPART%>"><c:out value="<%= cmpconfig.AUTHMODULE_DN_PART_PWD %>" /></label>
				&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
				<label for="<%=LIST_DNPARTS%>"><c:out value="<%= ejbcawebbean.getText(\"CMPAUTHSUBJECTDNPART\") %>" /></label>
				<select name="<%=LIST_DNPARTS%>" >
           				<% String currentDnPart = cmpconfig.getAuthenticationParameter(cmpconfig.AUTHMODULE_DN_PART_PWD, alias);
           				   Iterator authdnitr = dnfields.iterator();
           				   while(authdnitr.hasNext()) {
           				   		String dnpart = (String) authdnitr.next(); %>
           	           	   		<option  value='<c:out value="<%= dnpart %>"/>'  <% if(currentDnPart.equals(dnpart)) { %> selected <% } %> >
           	           	   			<c:out value="<%= dnpart %>"/> 
           	           	   		</option>
           	            <% } %>
        		</select>
		<% } %>
      </td>
    </tr>







    
    
	<% if(ramode) { %>
	<!-- ---------------------- RA MODE Specifics ----------------------------- -->
